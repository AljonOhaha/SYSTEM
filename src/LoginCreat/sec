    private void btnClearActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        pfPass.setText("");
    }                                        

    private void btnEnterActionPerformed(java.awt.event.ActionEvent evt) {                                         

        pass=pfPass.getText();
        if (pass.contains("12345")) {
            lblStats.setText("PASSWORD ACCEPTED! CLICK THE NEXT BUTTON");
        } 
        else 
        {
         lblStats.setText("PASSWORD INCORRECT:<");
    }                                        
    }
    private void btnNextActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        if (pass.contains("12345")) {
            lblStats.setText("PASSWORD ACCEPTED! CLICK THE NEXT BUTTON");
            new SalesEntry().setVisible(true);
        this.setVisible(false);
        } 
        else 
        {
         JOptionPane.showMessageDialog(this,"Invalid", "Error Login", JOptionPane.ERROR_MESSAGE);
         lblStats.setText("");
         pfPass.setText("");
    }   
        
    }                                       
    

